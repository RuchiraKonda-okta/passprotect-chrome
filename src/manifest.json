{
  "name": "PassProtect",
  "version": "0.1.8",
  "description": "Stop using bad passwords. PassProtect alerts you about breached credentials. Powered by \"Have I Been Pwned?\".",
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.min.js"]
    }
  ],
  "homepage_url": "https://www.passprotect.io",
  "offline_enabled": false,
  "permissions": [
    "storage"
  ]
}
